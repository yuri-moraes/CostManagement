    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
